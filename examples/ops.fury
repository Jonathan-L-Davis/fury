# byte operators
# arithmetic operators
operator byte + (byte a)+(byte b);
operator byte - (byte a)-(byte b);
operator byte * (byte a)*(byte b);
operator byte / (byte a)/(byte b);
operator byte % (byte a)%(byte b);
operator byte = (byte a)=(byte b);

# bitwise operators
operator byte ~         ~(byte b);
operator byte & (byte a)&(byte b);
operator byte | (byte a)|(byte b);
operator byte ^ (byte a)^(byte b);

# shift operators
operator byte >> (byte a)>>(byte b);
operator byte << (byte a)<<(byte b);
operator byte >O> (byte a)>O>(byte b);
operator byte <O< (byte a)<O<(byte b);

# comparison operators
operator byte == (byte a)==(byte b);
operator byte != (byte a)!=(byte b);
operator byte >= (byte a)>=(byte b);
operator byte >  (byte a)> (byte b);
operator byte <= (byte a)<=(byte b);
operator byte <  (byte a)< (byte b);



